{
  "name": "rosalana/roles",
  "description": "Rosalana package for managing user roles and permissions.",
  "type": "library",
  "keywords": [
    "laravel",
    "package",
    "rosalana",
    "roles",
    "permissions"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "michaelbany",
      "email": "mobany526@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/support": "^12.0",
    "rosalana/core": "^1 || dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0",
    "orchestra/testbench": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Rosalana\\Roles\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Rosalana\\Roles\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Rosalana\\Roles\\Providers\\RosalanaRolesServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
